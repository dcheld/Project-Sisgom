[module generateJava('http://www.eclipse.org/uml2/5.0.0/UML', 'http://www.eclipse.org/emf/2002/Ecore')/]
[import org::eclipse::mars::acceleo::module::sample::common/]
[import org::eclipse::mars::acceleo::module::sample::interfaceBody/]
[import org::eclipse::mars::acceleo::module::sample::classEntityBody/]
[import org::eclipse::mars::acceleo::module::sample::enumerationBody /]
[import org::eclipse::mars::acceleo::module::sample::uml2services /]

[template public generateClass(c : Class)]
[comment @main /]
[file (c.getFullPathFile().trim(), false)]
[_commentFileBlock()/]
[c.packageBlock()/]

[importBlock()/]

[_commentBodyBlock()/]
[if(c.hasStereotype('Entity'))]
[c.generateEntityClassBody()/]
[/if]
[/file]
[/template]

[template public generateInterface(i : Interface)]
[comment @main /]
[file (i.getFullPathFile().trim(), false)]
[_commentFileBlock()/]
[i.packageBlock()/]

[importBlock()/]

[_commentBodyBlock()/]
[i.generateInterfaceBody()/]
[/file]
[/template]

[template public generateEnumeration(e : Enumeration)]
[comment @main /]
[file (e.getFullPathFile().trim(), false)]
[_commentFileBlock()/]
[e.packageBlock()/]

[importBlock()/]

[_commentBodyBlock()/]
[e.generateEnumerationBody()/]
[/file]
[/template]